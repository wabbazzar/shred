{
  "exercises": [
    {
      "id": "bench-press",
      "name": "Bench Press",
      "aliases": ["barbell-bench-press", "flat-bench-press"],
      "category": "strength",
      "subcategory": "pressing",
      "muscleGroups": {
        "primary": ["chest", "triceps"],
        "secondary": ["front-delts"],
        "stabilizers": ["core", "legs"]
      },
      "equipment": {
        "required": ["barbell", "bench", "weight-plates"],
        "optional": ["safety-bars", "spotter"],
        "alternatives": {
          "barbell": ["dumbbells", "resistance-bands"],
          "bench": ["floor", "stability-ball"]
        }
      },
      "execution": {
        "setup": "Lie on bench with eyes under the bar, feet flat on floor, maintain natural arch",
        "movement": "Lower bar to chest with control, press up powerfully to full extension",
        "breathingPattern": "Inhale at top, hold breath during descent, exhale during press",
        "commonMistakes": ["bouncing off chest", "uneven bar path", "lifting feet off floor"],
        "cues": ["chest up", "drive through heels", "squeeze the bar", "push floor away"]
      },
      "parameters": {
        "trackingType": "weight-reps",
        "defaultSets": 4,
        "repRanges": {
          "strength": "1-5",
          "power": "3-6", 
          "hypertrophy": "6-12",
          "endurance": "12+"
        },
        "restPeriods": {
          "strength": "3-5 minutes",
          "hypertrophy": "90-180 seconds",
          "endurance": "60-90 seconds"
        },
        "tempo": "3-1-X-0"
      },
      "variations": [
        {
          "name": "Incline Bench Press",
          "difficulty": "same",
          "description": "30-45 degree incline targets upper chest",
          "equipment": ["incline-bench"],
          "focus": "upper-chest"
        },
        {
          "name": "Close-Grip Bench Press",
          "difficulty": "same",
          "description": "Narrow grip emphasizes triceps",
          "equipment": ["barbell", "bench"],
          "focus": "triceps"
        }
      ],
      "progressions": {
        "easier": ["push-ups", "incline-push-ups", "dumbbell-press"],
        "harder": ["pause-bench-press", "weighted-bench-press", "board-press"],
        "lateral": ["incline-bench-press", "decline-bench-press", "dumbbell-bench-press"]
      },
      "contraindications": ["shoulder-impingement", "recent-chest-surgery"],
      "difficulty": "intermediate",
      "unilateral": false
    },

  "workoutTemplate": {
    "1": {
      "type": "gym",
      "focus": "Chest & Biceps",
      "duration": "60-90 minutes",
      "sections": [
        {
          "name": "Warm-up",
          "exercises": [
            {
              "name": "Band Pull-Aparts",
              "category": "mobility",
              "sets": 1,
              "reps": "200",
              "notes": "Pre-activation for shoulders"
            },
            {
              "name": "Banded Shoulder Dislocations",
              "category": "mobility",
              "sets": 1,
              "reps": "100",
              "notes": "Shoulder mobility and activation"
            },
            {
              "name": "Bodyweight Chin/Pull-Ups",
              "category": "bodyweight",
              "sets": 4,
              "reps": "AMRAP",
              "notes": "Warm-up activation sets"
            }
          ]
        },
        {
          "name": "Main Lifts",
          "exercises": [
            {
              "name": "Flat Bench Press",
              "category": "strength",
              "sets": 7,
              "reps": ["4", "4", "4", "4", "4", "4", "4"],
              "notes": "Week 1: 4x4@170, 2x4@180, 1x4@185 | Week 2: 4x3@180, 2x3@190, 1x3@190, 1x3@195 | Week 3: 1x6@190 or 1x4@200, 1x2@210"
            },
            {
              "name": "Incline Bench Press",
              "category": "strength",
              "sets": 1,
              "reps": "10+ AMRAP",
              "notes": "Week 1: 10+@140, MRS 8+@125, EMOM 5 reps | Week 2: 8+@145, MRS 6+@135, EMOM 4 reps | Week 3: 12+@130, MRS 10+@115, EMOM 6 reps"
            }
          ]
        },
        {
          "name": "EMOM Work",
          "exercises": [
            {
              "name": "Incline Bench EMOM",
              "category": "emom",
              "time": "Until failure",
              "notes": "EMOM until failure to complete prescribed reps - varies by week"
            }
          ]
        }
      ]
    },

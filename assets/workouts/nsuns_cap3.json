{
  "id": "nsuns-cap3",
  "name": "nSuns CAP3 (Cyclical AMRAP Progression)",
  "description": "3-week cyclical AMRAP progression program for strength and muscle building",
  "version": "1.0.0",
  "weeks": 3,
  "daysPerWeek": 7,
  "metadata": {
    "targetAudience": "Powerlifting and strength training",
    "difficulty": "Advanced",
    "equipment": ["Full gym access", "Barbell", "Plates", "Bands"],
    "estimatedDuration": "60-90 minutes per session"
  },
  "dayTypes": {
    "1": "gym",
    "2": "gym", 
    "3": "gym",
    "4": "gym",
    "5": "rest",
    "6": "gym",
    "7": "gym"
  },
